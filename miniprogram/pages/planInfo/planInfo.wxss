.money-title{
  width: 100vw;
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 20px;
  color:#7890ad;
  box-sizing: border-box;
  border-bottom:1px solid #e5e5e5;
}
.money{
  width:98vw;
  max-height:calc(100vh - 40px);
  margin:auto;
  background: #fff;
  overflow: auto;
  display: flex;
  flex-wrap: wrap;
  align-items: flex-start;
}
.money-items{
  width: calc(14vw - 1px);
  height: 14vw;
  display:flex;
  justify-content:center;
  align-items:center;
  border-right: 1px solid;
  border-bottom:1px solid;
  border-color: #e5e5e5;
  font-size: 16px;
  font-weight: 700;
  color: #7890ad;
}
.money-items .done{
  width: calc(14vw - 1px);
  height: 14vw;
  display:flex;
  justify-content:center;
  align-items:center;
  position: relative;
}
.money-items .done .done-num{
  font-size: 12px;
  font-weight: 500;
  height: 12px;
  line-height: 12px;
  position: absolute;
  right: .5px;
  bottom: .5px;
}
.money-img{
  width: 70%;
  height: 70%;
}